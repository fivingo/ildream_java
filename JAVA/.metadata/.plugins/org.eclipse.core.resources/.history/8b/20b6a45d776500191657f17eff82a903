package day02;

public class Day02_03 {

	public static void main(String[] args) {
			
		System.out.println("1. 논리형 자료형");
		
		boolean bool1;
		bool1 = true;
		
		System.out.println("bool1 = " + bool1);
		
		boolean bool2 = false;
		
		System.out.println("bool2 = " + bool2);
		
		System.out.println("2. 정수형 자료형");
		
		byte by = 128;
		 System.out.println(by);
		
	}
}
